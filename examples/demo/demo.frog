var int n = 10;
var int sum = 0;

for (var int i = 1; i <= n; i = i + 1) {
    sum = sum + i;
}

print(sum);

func int factorial(int x) {
    if (x <= 1) {
        return 1;
    }
    return x * factorial(x - 1);
}

print(factorial(5));

